<Window x:Class="IDKin.IM.Windows.View.UpdateWindow" Name="updateWindow" Height="155" Width="400" Title="{DynamicResource UpdateWindowTitle}" Style="{DynamicResource WindowStyle}" ResizeMode="NoResize" ShowInTaskbar="False" Closing="Window_Closing" ContentRendered="updateWindow_ContentRendered" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Canvas>
    <Image Name="imgIcon" Width="64" Height="64" Stretch="Fill" Source="pack://application:,,,/IN;component/Resources/Icon/in.ico" Canvas.Left="12" Canvas.Top="12" />
    <TextBlock Name="lblMsg" Width="284" Canvas.Left="82" Canvas.Top="12" />
    <ProgressBar Name="progressBar" Canvas.Left="82" Canvas.Top="46" Width="284" Height="10" Minimum="0" Maximum="100" />
    <TextBlock Name="lblVer" Canvas.Left="12" Canvas.Top="86" Width="192" />
    <Button Name="btnCheckUpdate" Canvas.Left="291" Canvas.Top="66" Width="75" Height="42" Content="检查更新" Style="{DynamicResource NormalButtonStyle}" Click="CheckUpdate_Click" />
    <Button Name="btnYes" Canvas.Left="210" Canvas.Top="66" Content="更新" Height="42" Width="75" Visibility="Collapsed" Style="{DynamicResource NormalButtonStyle}" Click="btnYes_Click" />
    <Button Name="btnNo" Canvas.Left="291" Canvas.Top="66" Content="取消" Height="42" Width="75" Visibility="Collapsed" Style="{DynamicResource NormalButtonStyle}" Click="btnNo_Click" />
    <Button Name="btnExit" Canvas.Left="291" Canvas.Top="66" Content="取消" Height="42" Width="75" Visibility="Collapsed" Style="{DynamicResource NormalButtonStyle}" Click="btnExit_Click" />
  </Canvas>
</Window>